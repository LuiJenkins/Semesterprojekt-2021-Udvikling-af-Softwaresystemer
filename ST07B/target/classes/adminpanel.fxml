<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="admin_anchor" prefHeight="1000.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.AdminpanelController">
   <children>
      <Label layoutX="30.0" layoutY="24.0" prefHeight="58.0" prefWidth="282.0" text="Admin panel">
         <font>
            <Font name="Arial" size="50.0" />
         </font></Label>
      <Button fx:id="adminlogout_button" layoutX="564.0" layoutY="24.0" mnemonicParsing="false" onAction="#logout" prefHeight="25.0" prefWidth="126.0" text="Logud" />
      <Label layoutX="564.0" layoutY="53.0" text="Admin">
         <font>
            <Font size="15.0" />
         </font></Label>
      <ListView fx:id="user_list" layoutX="49.0" layoutY="274.0" onMouseClicked="#userListClicked" prefHeight="570.0" prefWidth="288.0" />
      <Label layoutX="49.0" layoutY="251.0" text="Brugere">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="delete_user" layoutX="193.0" layoutY="878.0" mnemonicParsing="false" onAction="#deleteUser" prefHeight="25.0" prefWidth="144.0" text="Slet bruger" />
      <Button fx:id="edit_user" layoutX="49.0" layoutY="878.0" mnemonicParsing="false" onAction="#editUser" prefHeight="25.0" prefWidth="144.0" text="Rediger bruger" />
      <Button fx:id="create_user" layoutX="565.0" layoutY="272.0" mnemonicParsing="false" onAction="#createUser" text="Opret bruger" visible="false" />
      <TextField fx:id="create_password" layoutX="512.0" layoutY="306.0" promptText="Password" />
      <TextField fx:id="create_username" layoutX="365.0" layoutY="306.0" promptText="Brugernavn" />
      <Button fx:id="accept" layoutX="365.0" layoutY="463.0" mnemonicParsing="false" onAction="#accept" prefHeight="25.0" prefWidth="149.0" text="Accepter" />
      <Button fx:id="cancel" layoutX="514.0" layoutY="463.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="149.0" text="Fortryd" />
      <RadioButton fx:id="choose_producer" layoutX="543.0" layoutY="359.0" mnemonicParsing="false" onAction="#chooseUserId" text="Producer" />
      <RadioButton fx:id="choose_maintainer" layoutX="543.0" layoutY="385.0" mnemonicParsing="false" onAction="#chooseUserId" text="Maintainer" />
      <RadioButton fx:id="choose_admin" layoutX="543.0" layoutY="409.0" mnemonicParsing="false" onAction="#chooseUserId" text="Admin" />
      <Label layoutX="563.0" layoutY="272.0" prefHeight="25.0" prefWidth="93.0" text="Opret Bruger">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
